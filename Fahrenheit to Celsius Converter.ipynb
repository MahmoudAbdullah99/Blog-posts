   "source": [
    "def fahrenheit_to_celsius():\n",
    "    temp = input(\"Enter temperature in Fahrenheit: \")\n",
    "    temp = float(temp)\n",
    "    celsius = (temp - 32) * 5/9\n",
    "    print(f\"{temp} in Fahrenheit is equal to {celsius} in Celsius\")"
   ]
  },
